{
  "compilerOptions": {
      "target": "ES2022",
      "module": "ES2022",
      "sourceMap": true,
      "lib": [
          "ES2022",
          "dom"
      ],
      "noImplicitAny": true,
      "removeComments": true,
      "strictNullChecks": true,
      "isolatedModules": true,
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "moduleResolution": "Bundler",
      "types": ["vite/client"],
  }
}